[{"_id":"MWRKn33RwQby2rb96","title":"Der Krötenpapst","author":"S. Edel","lines":[{"text":"Es sei von Nöten, dass die Kröten verböten, ihre Föten zu töten,","styles":["- none -"]},{"text":"sagte der Krötenpapst","styles":["- none -"]}],"createdAt":"2015-06-11T19:02:28.221Z","updatedAt":"2015-07-04T19:19:24.419Z","language":["de"],"numberOfLines":2},{"_id":"5YTLwAZqzNv5GP6JG","author":"Charles Bukowski","lines":[{"text":"our bones","styles":["- none -"]},{"text":"like stems into the sky","styles":["- none -"]},{"text":"will forever cry","styles":["- none -"]},{"text":"victory.","styles":["- none -"]}],"createdAt":"2015-06-21T16:11:14.938Z","language":["en"],"updatedAt":"2015-07-04T20:14:42.185Z","numberOfLines":4},{"_id":"xszouGTsyPeXYZKGu","title":"Wer nicht mehr liebt","author":"Johann Wolfgang von Goethe","lines":[{"text":"Wer nicht mehr liebt und nicht mehr irrt,","styles":["- none -"]},{"text":"der lasse sich begraben."}],"createdAt":"2015-06-19T23:47:45.461Z","updatedAt":"2015-07-04T19:20:00.174Z","language":["de"],"numberOfLines":2},{"_id":"rkKnLZvBfpFFNGjpE","title":"Abendlied","author":"Martin Walser","lines":[{"text":"Es gehen noch wenige Leute umher,","styles":["- none -"]},{"text":"ein einzelner noch, dann keiner mehr.","styles":["paragraph"]},{"text":"Es möchte sich legen auf Haus und Flur","styles":["- none -"]},{"text":"so etwas wie Müdigkeit der Natur.","styles":["paragraph"]},{"text":"Es lächelt so fein um Baum und Baum.","styles":["- none -"]},{"text":"Das Lächeln jedoch unterscheidet man kaum.","styles":["paragraph"]},{"text":"Was doch ein Windlein armselig ist,","styles":["- none -"]},{"text":"das noch am Abend die Welt bemißt.","styles":["paragraph"]},{"text":"Mich kommt ein Zögern und Schlafen an;","styles":["- none -"]},{"text":"ich betrachte nur noch den ernstesten Mann,","styles":["paragraph"]},{"text":"den Mond, der sehr an Bedeutung gewinnt,","styles":["- none -"]},{"text":"sobald die Sonne der Welt entrinnt.","styles":["- none -"]}],"createdAt":"2015-06-21T15:27:43.622Z","updatedAt":"2015-07-04T19:20:11.172Z","language":["de"],"numberOfLines":12},{"_id":"8j3Tt8Edm9gE66xaW","title":"Das Geben","author":"Johann Wolfgang von Goethe","lines":[{"text":"Sage nicht, dass du geben willst, sondern gib!","styles":["- none -"]}],"createdAt":"2015-06-19T23:49:47.980Z","language":["de"],"updatedAt":"2015-07-04T19:19:44.230Z","numberOfLines":1},{"_id":"jtnn6cjQvF8YpskwA","title":"?","author":"Charles Bukowski","lines":[{"text":"drunk now"},{"text":"at 4 a.m."},{"text":"I hear the sirens"},{"text":"of rainy white"},{"text":"ambulances,"},{"text":"then a dog"},{"text":"barks"},{"text":"in this tough-boy"},{"text":"Christ"},{"text":"morning."}],"createdAt":"2015-06-21T16:24:05.468Z","language":["en"],"updatedAt":"2015-07-04T19:20:49.968Z","numberOfLines":10},{"_id":"6WJ3Avf4Se7yzmWtW","author":"Kobayashi Issa","language":["de"],"numberOfLines":3,"lines":[{"text":"Auf dem Seerosenblatt der Frosch"},{"text":"aber was macht er"},{"text":"für ein Gesicht?"}],"createdAt":"2015-07-04T19:58:03.352Z","updatedAt":"2015-07-04T20:14:12.378Z"},{"_id":"bqgNkzJu8oqyfEYW7","author":"Matsuo Bashou","language":["en"],"reference":"http://kisaragichiyo.deviantart.com/art/Haiku-Furuike-ya-185104018","numberOfLines":3,"lines":[{"text":"An old silent pond..."},{"text":"A frog jumps into the pond,"},{"text":"splash! Silence again."}],"createdAt":"2015-07-04T20:00:37.172Z","updatedAt":"2015-07-04T20:14:22.835Z"},{"_id":"R6dN5BzqtEpzrZbAT","title":"Abendlied","author":"Goethe","lines":[{"text":"Über allen Gipfeln"},{"text":"Ist Ruh,"},{"text":"In allen Wipfeln"},{"text":"Spürest du"},{"text":"Kaum einen Hauch;"},{"text":"Die Vögelein schweigen im Walde."},{"text":"Warte nur, balde"},{"text":"Ruhest du auch."}],"createdAt":"2015-06-20T11:45:32.659Z","language":["de"],"updatedAt":"2015-07-04T19:21:06.312Z","numberOfLines":8},{"_id":"a3NMwLyQZMxKeK5Wi","author":"Matsuo Bashō","year":"1677","reference":"http://kulturserver-nds.de/home/haiku-dhg/Archiv/Sommergras76-Wenzel1.pdf","numberOfLines":3,"lines":[{"text":"Die läufige Katze"},{"text":"schlüpft durch den Riss an der Feuerstelle","styles":["- none -"]},{"text":"hinein und hinaus","styles":["- none -"]}],"createdAt":"2015-07-04T20:09:30.001Z"},{"_id":"DZRNs7oMeDJeYgJF7","author":"Matsuo Bashō","language":["de"],"year":"1684","reference":"http://kulturserver-nds.de/home/haiku-dhg/Archiv/Sommergras76-Wenzel1.pdf","numberOfLines":3,"lines":[{"text":"Am Straßenrand","styles":["- none -"]},{"text":"die Sharonrose, von meinem Pferd","styles":["- none -"]},{"text":"gefressen","styles":["- none -"]}],"createdAt":"2015-07-04T22:11:34.427Z"},{"_id":"WpHXxLsHMz7kCqWr3","title":"An Luise","author":"Joseph Freiherr von Eichendorff","language":["de"],"reference":"http://gedichte.xbib.de/Eichendorff_gedicht_An+Luise.htm","numberOfLines":10,"lines":[{"text":"Ich wollt in Liedern oft dich preisen,","styles":["- none -"]},{"text":"Die wunderstille Güte,","styles":["- none -"]},{"text":"Wie du ein halbverwildertes Gemüte","styles":["- none -"]},{"text":"Dir liebend hegst und heilst auf tausend süße Weisen,","styles":["- none -"]},{"text":"Des Mannes Unruh und verworrnem Leben","styles":["- none -"]},{"text":"Durch Tränen lächelnd bis zum Tod ergeben.","styles":["- none -"]},{"text":"So schön still in stillem Harme","styles":["- none -"]},{"text":"Sitzt du vor mir, das Kindlein auf dem Arme,","styles":["- none -"]},{"text":"Im blauen Auge Treu und Frieden ohne Ende,","styles":["- none -"]},{"text":"Ach, wen Gott lieb hat, gab er solche Fraue!","styles":["- none -"]}],"createdAt":"2015-07-04T22:26:48.483Z"},{"_id":"xP9RLhuqyCMEDNg9E","title":"Andre haben andre Schwingen","author":"Joseph Freiherr von Eichendorff","language":["de"],"reference":"http://gedichte.xbib.de/Eichendorff_gedicht_Andre+haben+andre+Schwingen.htm","numberOfLines":4,"lines":[{"text":"Andre haben andre Schwingen,","styles":["- none -"]},{"text":"Aber wir, mein fröhlich Herz,","styles":["- none -"]},{"text":"Wollen grad hinauf uns singen,","styles":["- none -"]},{"text":"Aus dem Frühling himmelwärts!","styles":["- none -"]}],"createdAt":"2015-07-04T22:28:21.987Z"},{"_id":"3tjRvKSKYrYSoGDSk","title":"Aussicht","author":"Joseph Freiherr von Eichendorff","language":["de"],"reference":"http://gedichte.xbib.de/Eichendorff_gedicht_Aussicht.htm","lines":[{"text":"Komm zum Garten denn, du Holde!"},{"text":"In den warmen, schönen Tagen","styles":["- none -"]},{"text":"Sollst du Blumenkränze tragen,","styles":["- none -"]},{"text":"Und vom kühl kristallnen Golde","styles":["- none -"]},{"text":"Mit den frischen, roten Lippen,","styles":["paragraph"]},{"text":"Eh ich trinke, lächelnd nippen.","styles":["- none -"]},{"text":"Ohne Maß dann, ohne Richter,","styles":["- none -"]},{"text":"Küssend, trinkend singt der Dichter","styles":["- none -"]},{"text":"Lieder, die von selbst entschweben:","styles":["- none -"]},{"text":"Wunderschön ist doch das Leben!","styles":["- none -"]}],"createdAt":"2015-07-04T22:31:48.705Z","numberOfLines":10},{"_id":"WJtjiKYqu7X82G6fb","title":"Ahnung","author":"Heinrich Heine","language":["de"],"reference":"http://gedichte.xbib.de/Heine_gedicht_Ahnung.htm","numberOfLines":6,"lines":[{"text":"Oben, wo die Sterne glühen,","styles":["- none -"]},{"text":"Müssen uns die Freuden blühen,","styles":["- none -"]},{"text":"Die uns unten sind versagt;","styles":["- none -"]},{"text":"In des Todes kalten Armen","styles":["- none -"]},{"text":"Kann das Leben erst erwarmen,","styles":["- none -"]},{"text":"Und das Licht der Nacht enttagt.","styles":["- none -"]}],"createdAt":"2015-07-04T22:38:06.303Z"},{"_id":"JxH9YhQy7vuwur9C3","title":"Finis","author":"Georg Heym","language":["de"],"reference":"http://gedichte.xbib.de/Heym_gedicht_Finis.htm","lines":[{"text":"Frühe Veilchen gab mir deine Hand,"},{"text":"Drum sie schlang ein schon verblaßtes Band,","styles":["- none -"]},{"text":"Sie bewahrt an spät besonnter Wand","styles":["- none -"]},{"text":"Schmaler Rahmen für des Alters Land.","styles":["- none -"]}],"createdAt":"2015-07-04T22:41:29.256Z","numberOfLines":4},{"_id":"KKcNc6aoarkanuEXn","title":"Meine Seele","author":"Georg Heym","reference":"http://gedichte.xbib.de/Heym_gedicht_Meine+Seele.htm","lines":[{"text":"Meine Seele ist eine Schlange,"},{"text":"Die ist schon lange tot,","styles":["- none -"]},{"text":"Nur manchmal in Herbstesmorgen,","styles":["- none -"]},{"text":"Entblättertem Abendrot","styles":["- none -"]},{"text":"Wachse ich steil aus dem Fenster,","styles":["- none -"]},{"text":"Wo fallende Sterne sind,","styles":["- none -"]},{"text":"Über den Blumen und Kressen","styles":["- none -"]},{"text":"Meine Stirne spiegelt","styles":["- none -"]},{"text":"Im stöhnenden Nächte-Wind.","styles":["- none -"]}],"createdAt":"2015-07-04T22:43:00.907Z","numberOfLines":9},{"_id":"dg6GLYcDyzxuBAmmH","author":"Hilde Domin","language":["de"],"reference":"https://de.wikipedia.org/wiki/Poem_%E2%80%93_Ich_setzte_den_Fu%C3%9F_in_die_Luft_und_sie_trug","numberOfLines":1,"lines":[{"text":"Ich setzte den Fuß in die Luft und sie trug","styles":["- none -"]}],"createdAt":"2015-07-04T22:47:18.996Z"},{"_id":"RM8mYX8NzwvhEoZpZ","title":"Gegengewicht","author":"Hilde Domin","language":["de"],"reference":"Hilde Domin/Gesammelte Gedichte/S.Fischer/s.81, http://web.zhbluzern.ch/veranstaltungen/2005/dok/Hilde_Domin_Gedichte.pdf","numberOfLines":8,"lines":[{"text":"Wie kann ich","styles":["- none -"]},{"text":"in meinem blauesten Kleid","styles":["- none -"]},{"text":"und riefe ich alle die blühenden Zweige","styles":["- none -"]},{"text":"und alle Nachtigallen zu Hilfe","styles":["paragraph"]},{"text":"wie kann ich mit Lachen oder mit Tränen","styles":["- none -"]},{"text":"das Gleichgewicht halten","styles":["- none -"]},{"text":"in der die Welt liegt","styles":["- none -"]},{"text":"eine Nuss aus Blei?","styles":["- none -"]}],"createdAt":"2015-07-04T23:00:34.368Z","updatedAt":"2015-07-17T09:18:48.430Z"},{"_id":"Kj5Gqpjhr5EeigQht","title":"Alle meine Schiffe","author":"Hilde Domin","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/alle-meine-schiffe.html","lines":[{"text":"Alle meine Schiffe"},{"text":"haben die Häfen vergessen","styles":["- none -"]},{"text":"und meine Füße den Weg.","styles":["- none -"]},{"text":"Es wird nicht gesät und nicht geerntet","styles":["- none -"]},{"text":"denn es ist keine Vergangenheit","styles":["- none -"]},{"text":"und keine Zukunft,","styles":["- none -"]},{"text":"kaum eine Bühne im Tag.","styles":["- none -"]},{"text":"Nur der kleine","styles":["- none -"]},{"text":"zärtliche Abstand","styles":["- none -"]},{"text":"zwischen dir und mir,","styles":["- none -"]},{"text":"den du nicht verminderst.","styles":["- none -"]}],"createdAt":"2015-07-04T23:04:48.196Z","numberOfLines":11},{"_id":"o9zTaJfzG3G8pfeX7","title":"Mein Geschlecht zittert","author":"Hilde Domin","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/mein-geschlecht-zittert.html","lines":[{"text":"Mein Geschlecht zittert"},{"text":"wie ein Vögelchen","styles":["- none -"]},{"text":"unter dem","styles":["- none -"]},{"text":"Griff deines Blicks.","styles":["paragraph"]},{"text":"Deine Hände eine zärtliche Brise","styles":["- none -"]},{"text":"auf meinem Leib.","styles":["- none -"]},{"text":"Alle meine Wachen","styles":["- none -"]},{"text":"fliehn.","styles":["paragraph"]},{"text":"Du öffnest die letzte Tür.","styles":["- none -"]},{"text":"Ich bin so erschrocken","styles":["- none -"]},{"text":"daß aller Schlaf","styles":["- none -"]},{"text":"dünn wird","styles":["- none -"]},{"text":"wie ein zerschlissenes Tuch.","styles":["- none -"]}],"createdAt":"2015-07-04T23:09:45.878Z","numberOfLines":13},{"_id":"SfZjdcbjJoAuHo5td","title":"Nicht müde werden","author":"Hilde Domin","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/nicht-muede-werden.html","lines":[{"text":"Nicht müde werden"},{"text":"sondern dem Wunder","styles":["- none -"]},{"text":"leise","styles":["- none -"]},{"text":"wie einem Vogel","styles":["- none -"]},{"text":"die Hand hinhalten.","styles":["- none -"]}],"createdAt":"2015-07-04T23:10:49.021Z","numberOfLines":5},{"_id":"qsi7qKfFKgegnkcuq","title":"Abendständchen","author":"Clemens Brentano","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/abendstaendchen.html","lines":[{"text":"Hör, es klagt die Flöte wieder,"},{"text":"und die kühlen Brunnen rauschen!","styles":["- none -"]},{"text":"Golden weh'n die Töne nieder,","styles":["- none -"]},{"text":"stille, stille, laß uns lauschen!","styles":["paragraph"]},{"text":"Holdes Bitten, mild Verlangen,","styles":["- none -"]},{"text":"wie es süß zum Herzen spricht!","styles":["- none -"]},{"text":"Durch die Nacht, die mich umfangen,","styles":["- none -"]},{"text":"blickt zu mir der Töne Licht!","styles":["- none -"]}],"createdAt":"2015-07-04T23:15:13.616Z","numberOfLines":8},{"_id":"NCiEr6ecEjiXSpFC5","title":"Die Ameisen","author":"Joachim Ringelnatz","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/die-ameisen-866.html","lines":[{"text":"In Hamburg lebten zwei Ameisen,"},{"text":"Die wollten nach Australien reisen.","styles":["- none -"]},{"text":"Bei Altona auf der Chaussee,","styles":["- none -"]},{"text":"Da taten ihnen die Beine weh,","styles":["- none -"]},{"text":"Und da verzichteten sie weise","styles":["- none -"]},{"text":"Dann auf den letzten Teil der Reise.","styles":["- none -"]}],"createdAt":"2015-07-04T23:18:23.363Z","numberOfLines":6},{"_id":"MektkCGXRCza6zWbQ","title":"Bumerang","author":"Joachim Ringelnatz","language":["de"],"reference":"http://www.deutschelyrik.de/index.php/bumerang.html","lines":[{"text":"War einmal ein Bumerang;"},{"text":"War ein Weniges zu lang.","styles":["- none -"]},{"text":"Bumerang flog ein Stück,","styles":["- none -"]},{"text":"Aber kam nicht mehr zurück.","styles":["- none -"]},{"text":"Publikum ­ noch stundenlang ­","styles":["- none -"]},{"text":"Wartete auf Bumerang.","styles":["- none -"]}],"createdAt":"2015-07-04T23:21:12.524Z","numberOfLines":6},{"_id":"2KhyaQFYigba8D8Rw","author":"Jakob Daniel Hahnenknoop","language":["de"],"reference":"http://www.literatur-niedersachsen.de/landgang/detailansicht/der-fuenfzeiler-aus-limmer-und-ricklingen.html","lines":[{"text":"Das niedliche Örtchen Ricklingen,"},{"text":"Das wollte ein Dichter besingen.","styles":["- none -"]},{"text":"Doch dann zog er nach Hessen,","styles":["- none -"]},{"text":"Da hat er's vergessen.","styles":["- none -"]},{"text":"So mußte die Sache mißlingen.","styles":["- none -"]}],"createdAt":"2015-07-04T23:31:15.381Z","numberOfLines":5},{"_id":"JquFC2FbeGETzoTxp","author":"Jakob Daniel Hahnenknoop","language":["de"],"reference":"http://www.literatur-niedersachsen.de/landgang/detailansicht/der-fuenfzeiler-aus-limmer-und-ricklingen.html","lines":[{"text":"Ein kleiner Franzose aus Reims"},{"text":"Studierte tagtäglich die Times.","styles":["- none -"]},{"text":"Zwar verstand er kein Britisch,","styles":["- none -"]},{"text":"Doch das sah er nicht kritisch:","styles":["- none -"]},{"text":"Die Times war ihm Zier seines Heims.","styles":["- none -"]}],"createdAt":"2015-07-04T23:32:15.952Z","numberOfLines":5},{"_id":"yT6RErvonGxzL2DLN","title":"Wenn im Unendlichen dasselbe","author":"Johann Wolfgang von Goethe","language":["de"],"reference":"http://gedichte.xbib.de/Goethe_gedicht_Wenn+im+Unendlichen+dasselbe.htm","lines":[{"text":"Wenn im Unendlichen dasselbe","styles":["- none -"]},{"text":"Sich wiederholend ewig fließt,"},{"text":"Das tausendfältige Gewölbe"},{"text":"Sich kräftig ineinander schließt,"},{"text":"Strömt Lebenslust aus allen Dingen,"},{"text":"Dem kleinsten wie dem größten Stern,"},null,{"text":"Und alles Drängen, alles Ringen","styles":["- none -"]},{"text":"Ist ewige Ruh in Gott dem Herrn.","styles":["- none -"]}],"createdAt":"2015-07-04T23:40:28.845Z","numberOfLines":9,"updatedAt":"2015-07-04T23:42:07.734Z"},{"_id":"Refvf95z2gBAxuSgu","author":"Frida Kahlo","language":["en"],"year":"http://psychoactivedreams.tumblr.com/image/118177598353","numberOfLines":5,"lines":[{"text":"Nothing is absolute,","styles":["- none -"]},{"text":"everything changes,","styles":["- none -"]},{"text":"everything moves,","styles":["- none -"]},{"text":"everything revolutionizes,","styles":["- none -"]},{"text":"all flies and goes.","styles":["- none -"]}],"createdAt":"2015-07-11T19:52:48.141Z"},{"_id":"5WEAjQeXxzyLq2vNr","author":"Andreas Gryphius","reference":"http://wortwuchs.net/zweizeiler/","numberOfLines":2,"lines":[{"text":"Ungläubigen die Wahrheit preisen,","styles":["- none -"]},{"text":"heißt Blinden schöne Dirnen weisen.","styles":["- none -"]}],"createdAt":"2015-07-13T14:14:15.746Z"},{"_id":"MHEfMXRB2h8NyLAP4","author":"Friedrich Rückert","language":["de"],"numberOfLines":2,"lines":[{"text":"Der Adler fliegt allein; der Rabe scharenweise.","styles":["- none -"]},{"text":"Gesellschaft braucht der Tor, und Einsamkeit der Weise.","styles":["- none -"]}],"createdAt":"2015-07-13T14:16:42.225Z"},{"_id":"G7tWZf5v3NitZCsFW","title":"Das Kind in der Wiege","author":"Friedrich Schiller","language":["de"],"reference":"http://www.friedrich-schiller-archiv.de/gedichte-schillers/zweizeiler/das-kind-in-der-wiege/#sthash.0DnU9nz7.dpuf","lines":[{"text":"Glücklicher Säugling! Dir ist ein unendlicher Raum noch die Wiege,"},{"text":"Werde Mann, und dir wird eng die unendliche Welt.","styles":["- none -"]}],"createdAt":"2015-07-13T14:34:31.392Z","numberOfLines":2},{"_id":"2aPxpWovmx6KdC7pn","title":"Würde des Menschen","author":"Friedrich Schiller","language":["de"],"lines":[{"text":"Nichts mehr davon, ich bitt euch. Zu essen gebt ihm, zu wohnen,"},{"text":"Habt ihr die Blöße bedeckt, gibt sich die Würde von selbst.","styles":["- none -"]}],"createdAt":"2015-07-13T14:37:40.651Z","numberOfLines":2},{"_id":"nD9TQ8kCW6fXXqnE7","author":"Alan J. Perlis","language":["en"],"reference":"http://pu.inf.uni-tuebingen.de/users/klaeren/epigrams.html","lines":[{"text":"Syntactic sugar causes cancer of the semi-colons.","styles":["- none -"]}],"createdAt":"2015-07-13T14:47:27.486Z","numberOfLines":1,"year":"1982","updatedAt":"2015-07-13T14:47:53.803Z"},{"_id":"Xkj4r9XPYheGLyoTK","title":"1.","author":"George MacDonald","year":"~ 1880","numberOfLines":7,"lines":[{"text":"LORD, what I once had done with youthful might,","styles":["- none -"]},{"text":"Had I been from the first true to the truth,"},{"text":"Grant me, now old, to do—with better sight,"},{"text":"And humbler heart, if not the brain of youth;"},{"text":"So wilt thou, in thy gentleness and ruth,"},{"text":"Lead back thy old soul, by the path of pain,"},{"text":"Round to his best—young eyes and heart and brain."}],"createdAt":"2015-07-13T14:55:16.632Z","language":["en"],"updatedAt":"2015-07-13T14:55:28.969Z"}]